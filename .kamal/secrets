SECRETS=$(kamal secrets fetch --adapter 1password --account ABOVBJHUF5G2PASRYQTVCYIJKM --from Personal/keystone_base RAILS_MASTER_KEY KAMAL_REGISTRY_PASSWORD SMTP_EMAIL_ADDRESS SMTP_PASSWORD SMTP_ADDRESS SMTP_PORT SMTP_DOMAIN TWILIO_ACCOUNT_SID)

KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD ${SECRETS})
